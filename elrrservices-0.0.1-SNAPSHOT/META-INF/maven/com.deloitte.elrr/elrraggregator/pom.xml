		<artifactId>spring-boot-starter-parent</artifactId>
		<version>2.7.6</version>
		<relativePath/> <!-- lookup parent from repository -->
	</parent>
	<groupId>com.deloitte.elrr</groupId>
	<artifactId>elrraggregator</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>elrraggregator</name>
	<description>ELRR Consolidate project</description>
	<properties>
		<java.version>11</java.version>
		<docker.image.prefix>elrrexternalservices</docker.image.prefix>
		<jacoco.version>0.8.6</jacoco.version>
		<start-class>com.deloitte.elrr.elrrconsolidate.ElrrConsolidateApplication</start-class>
		<jacoco.reportPath>${project.basedir}/../target/jacoco.exec</jacoco.reportPath>
		<sonar.java.coveragePlugin>jacoco</sonar.java.coveragePlugin>
		<sonar.dynamicAnalysis>resuseReports</sonar.dynamicAnalysis>
		<sonar.jacoco.reportPath>${project.basedir}/../target/jacoco.exec</sonar.jacoco.reportPath>
		<sona.lauguage>java</sona.lauguage>
	</properties>
